{
  "name": "@simple-contacts/mysql-js",
  "version": "1.0.1",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src",
    "tdd": "jest src --watch",
    "test": "yarn run -s lint && yarn run -s flow && jest src",
    "flow": "flow --include-warnings",
    "slurp": "sh -c '(cd ../simplecontacts/migrations && (ls | sort -n | while read f; do echo; echo \"--\"; echo \"-- $f\"; cat \"$f\"; echo; done))' > input.sql",
    "parse": "bin/parse-sql < input.sql > ast.json",
    "simulate": "babel-node src/runner.js",
    "all": "yarn parse && yarn simulate"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "pegjs",
      "doc"
    ]
  },
  "devDependencies": {
    "@simple-contacts/babel-preset-sc": "^1.0.0",
    "@simple-contacts/eslint-config": "^0.1.6",
    "@simple-contacts/sc-scripts": "^0.4.11",
    "flow-bin": "^0.68.0",
    "flow-typed": "^2.4.0",
    "import-sort-cli": "^4.2.0",
    "import-sort-parser-babylon": "^4.2.0",
    "import-sort-style-eslint": "^4.2.0",
    "import-sort-style-module": "^4.2.0",
    "jest-cli": "^23.0.0-alpha.0"
  },
  "dependencies": {
    "chalk": "^2.3.2",
    "immer": "^1.2.0",
    "lodash": "^4.17.5",
    "pegjs": "^0.10.0"
  }
}
